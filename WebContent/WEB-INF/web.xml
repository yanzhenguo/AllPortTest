<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>GreatProject</display-name>
  <listener>
		<listener-class>cn.shu.ClientListener</listener-class>
	</listener>
	<servlet>
	    <servlet-name>getjson</servlet-name>
	    <servlet-class>cn.shu.servlet.GetJson</servlet-class>
	</servlet>
  <servlet-mapping>
		<servlet-name>getjson</servlet-name>
		<url-pattern>/getjson</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>getRecent</servlet-name>
	    <servlet-class>cn.shu.servlet.GetRecent</servlet-class>
	</servlet>
  <servlet-mapping>
		<servlet-name>getRecent</servlet-name>
		<url-pattern>/GetRecent</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>getGates</servlet-name>
	    <servlet-class>cn.shu.servlet.GetGatesList</servlet-class>
	</servlet>
  <servlet-mapping>
		<servlet-name>getGates</servlet-name>
		<url-pattern>/GetGatesList</url-pattern>
	</servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>